{
  "name": "@antv/vis-steg",
  "version": "0.1.0",
  "description": "Visualization Steganography: conceal information within visualization images.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "browser": "dist/index.js",
  "module": "es/src/index.js",
  "files": [
    "src",
    "es",
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf lib && tsc --pretty",
    "start": "webpack serve --port 8812",
    "module": "rm -rf es && tsc -t ES5 -m ESNext --outDir es",
    "pack": "webpack --config=webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"demo/**/*.ts\" \"demo/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublish": "npm run module && npm run pack",
    "prepublishOnly": "npm run prepublish && npm test && npm run lint && webpack --mode=production",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/vis-steg.git"
  },
  "author": "AFX",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
