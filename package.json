{
  "name": "@antv/vis-steg",
  "version": "0.1.0",
  "description": "Visualization Steganography: conceal information within visualization images.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "browser": "dist/index.js",
  "module": "es/src/index.js",
  "files": [
    "src",
    "es",
    "dist",
    "lib"
  ],
  "scripts": {
    "start": "cd demo && webpack serve --port 8817",
    "module": "rm -rf es && tsc -t ES5 -m ESNext --outDir es",
    "pack": "webpack --config=webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"demo/**/*.{ts,tsx}\"",
    "lint-staged": "lint-staged",
    "size": "limit-size",
    "lint": "eslint ./src/**/*.ts ./__tests__/**/*.ts ./demo/**/*.{ts,tsx} && prettier ./src ./__tests__ ./demo --check ",
    "test": "jest",
    "build": "rm -rf lib && tsc --pretty",
    "hey": "npm run module && npm run pack && npm test && npm run lint && webpack --mode=production",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "ci": "run-s lint test build",
    "prepublishOnly": "npm run ci",
    "postinstall": "cd demo && npm install"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.22",
    "@types/jest-json-schema": "^2.1.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "browser-resolve": "^2.0.0",
    "classnames": "^2.3.1",
    "css-loader": "^5.2.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-json-schema": "^5.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "limit-size": "^0.1.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.4",
    "ts-json-schema-generator": "^0.89.2",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "limit-size": [
    {
      "path": "dist/bundle.js",
      "limit": "8 Kb",
      "gzip": true
    },
    {
      "path": "dist/bundle.js",
      "limit": "24 Kb"
    }
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/vis-steg.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/antvis/vis-steg/issues"
  },
  "homepage": "https://github.com/antvis/vis-steg#readme",
  "dependencies": {}
}
