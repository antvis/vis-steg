    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@types/jest": "^26.0.22",
    "@types/jest-json-schema": "^2.1.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "antd": "^4.15.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
